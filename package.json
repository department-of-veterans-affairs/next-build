{
  "name": "next-build",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup-dev.js",
    "dev": "yarn setup && SSG=false next dev",
    "build": "SSG=true next build",
    "build:preview": "SSG=false next build",
    "build:sitemap": "next-sitemap",
    "start": "next start",
    "export": "SSG=true next build && next export",
    "export:serve": "http-server out -p 8001",
    "redis": "docker run --name next-redis -p 6379:6379 -d redis",
    "redis:cli": "docker exec -it next-redis redis-cli",
    "redis:stop": "docker stop next-redis && docker rm next-redis",
    "format": "prettier --write .",
    "lint": "next lint",
    "postinstall": "husky install",
    "typedoc": "typedoc",
    "typedoc:serve": "http-server typedocs -p 8002",
    "test": "APP_ENV=test jest",
    "test:ci": "APP_ENV=test jest --ci --coverage",
    "test:coverage": "RUST_BACKTRACE=1 APP_ENV=test jest --coverage",
    "test:playwright": "playwright test --project=main",
    "test:playwright:interactive": "playwright test --ui",
    "test:playwright:a11y": "playwright test --project=a11y",
    "test:examples": "APP_ENV=test jest --rootDir ./example_tests --config ./example_tests/jest.config.js",
    "test:example": "APP_ENV=test yarn test:examples example_tests/${1}/",
    "test:debug": "APP_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:format": "APP_ENV=test prettier --check .",
    "test:types": "APP_ENV=test tsc --noEmit",
    "test:watch": "APP_ENV=test jest --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o out/storybook-static"
  },
  "dependencies": {
    "@department-of-veterans-affairs/component-library": "^31.0.0",
    "@department-of-veterans-affairs/formation": "^7.0.9",
