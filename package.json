{
  "name": "next-build",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup-dev.js",
    "dev": "yarn setup && SSG=false next dev",
    "build": "SSG=true next build",
    "postbuild": "next-sitemap",
    "export": "SSG=true next build && next export",
    "export:serve": "http-server out -p 8001",
    "format": "prettier --write .",
    "lint": "next lint",
    "postinstall": "husky install",
    "test": "APP_ENV=test jest",
    "test:ci": "APP_ENV=test jest --ci --coverage",
    "test:coverage": "RUST_BACKTRACE=1 APP_ENV=test jest --coverage",
    "test:cypress": "APP_ENV=test cypress run --spec \"cypress/e2e/**/*,!cypress/e2e/examples/**/*,!cypress/e2e/a11y.cy.js\"",
    "test:cypress:interactive": "APP_ENV=test cypress open",
    "test:cypress:a11y": "APP_ENV=test cypress run --spec \"cypress/e2e/a11y.cy.js\"",
    "test:cypress:examples": "APP_ENV=test cypress run --spec \"cypress/e2e/examples/**/*\"",
    "test:examples": "APP_ENV=test jest --rootDir ./example_tests --config ./example_tests/jest.config.js",
    "test:example": "APP_ENV=test yarn test:examples example_tests/${1}/",
    "test:debug": "APP_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:format": "APP_ENV=test prettier --check .",
    "test:types": "APP_ENV=test tsc --noEmit",
    "test:watch": "APP_ENV=test jest --watch",
    "gha:test:ci": "APP_ENV=test start-server-and-test export:serve http://http://127.0.0.1:8001 test:cypress",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@department-of-veterans-affairs/component-library": "^10.3.0",
    "@department-of-veterans-affairs/formation": "^7.0.1",
    "@department-of-veterans-affairs/web-components": "^4.5.2",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@storybook/react": "^7.2.1",
    "@types/react-gtm-module": "^2.0.1",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "drupal-jsonapi-params": "^2.0.0",
    "jest-axe": "^6.0.0",
    "jest-extended": "^4.0.1",
    "jsona": "^1.11.0",
    "lodash": "^4.17.21",
    "mq-polyfill": "^1.1.8",
    "next": "^13.4.12",
    "next-drupal": "^1.4.0",
    "next-drupal-query": "^0.2.1",
    "next-sitemap": "^4.2.2",
    "p-retry": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "safe-json-stringify": "^1.2.0",
    "socks-proxy-agent": "^6.2.0",
    "superjson": "^1.9.1",
    "swr": "^1.3.0",
    "syswide-cas": "^5.3.0",
    "uswds": "^2.13.3",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
