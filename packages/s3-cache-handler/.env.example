# S3 Cache Handler Environment Variables

# Enable S3 cache handler
USE_S3_CACHE=true

# AWS Configuration
AWS_REGION=us-gov-west-1
# AWS_ACCESS_KEY_ID=your-access-key-here
# AWS_SECRET_ACCESS_KEY=your-secret-key-here

# S3 Bucket Configuration
S3_CACHE_BUCKET_NAME=next-content.www.va.gov
S3_CACHE_KEY_PREFIX=next-cache

# Debug logging (set to true to see cache operations)
S3_CACHE_DEBUG=false
