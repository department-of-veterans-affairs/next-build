    {
      "type": "paragraph--basic_accordion",
      "id": "8fe7bd80-43c2-41ff-a199-84da7743faf9",
      "drupal_internal__id": 157078,
      "drupal_internal__revision_id": 1807367,
      "langcode": "en",
      "status": true,
      "created": "2024-07-09T16:26:46+00:00",
      "parent_id": "4328",
      "parent_type": "node",
